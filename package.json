{
  "name": "spanish-countries-capitals",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 5174",
    "css:build": "tailwindcss -i ./src/styles/input.css -o ./public/output.css --minify",
    "css:watch": "tailwindcss -i ./src/styles/input.css -o ./public/output.css --watch",
    "latex:build": "latexmk -pdf -interaction=nonstopmode -output-directory=worksheets worksheets/worksheet.tex",
    "latex:clean": "latexmk -C -output-directory=worksheets"
  },
  "dependencies": {
    "jspdf": "^2.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "vite": "^5.4.8"
  }
} 